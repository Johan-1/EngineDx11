Texture2D    Texture; 
SamplerState SampleType[2]; // wrap,clamp 	

struct PixelInputType
{
    float4 position : SV_POSITION;
    float2 tex      : TEXCOORD0;		
};

float4 Main(PixelInputType input) : SV_TARGET
{            
	return Texture.Load(int3(input.position.xy,0));	
}






























